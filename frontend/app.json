{
  "expo": {
    "name": "instagram",
    "slug": "instagram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.instagram.jimymltta",

      "config": {
        "googleSignIn": {
          "apiKey": "****"
        }
      }
    },
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "bundleIdentifier": "com.instagram.jimymltta",
      "config": {
        "googleSignIn": {
          "reservedClientId": "****"
        },
        "googleMapsApiKey": "****"
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    }
  }
}
